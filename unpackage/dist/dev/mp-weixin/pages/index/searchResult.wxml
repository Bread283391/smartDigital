<view wx:if="{{a}}" class="content"><view class="top-bar"><label class="top-text">搜索：{{b}}</label></view><view wx:if="{{c}}" class="class-box"><view wx:for="{{d}}" wx:for-item="value"><view class="card" bindtap="{{value.q}}"><view class="image-box"><image src="{{value.a}}" alt="" class="image" mode="aspectFill"></image></view><view class="text-box"><view class="text-line1 text-line"><label class="text-class-name">{{value.b}}</label></view><view class="text-line2 text-line"><label class="text-classes">{{value.c}}</label></view><view class="text-line3 text-line"> 讲师：<label class="text-teacher">{{value.d}}</label></view><view class="text-line4 text-line"><label class="text-time">{{value.e}}~{{value.f}}</label></view><view wx:if="{{false}}" class="button-box"><button wx:if="{{value.g}}" class="button button1" bindtap="{{value.h}}">报名学习</button><button wx:elif="{{value.i}}" class="button button2" bindtap="{{value.j}}">开始学习</button><button wx:elif="{{value.k}}" class="button button3" bindtap="{{value.l}}">未开始</button><button wx:elif="{{value.m}}" class="button button4" bindtap="{{value.n}}">已结束</button></view></view><label wx:if="{{value.o}}" class="price">￥{{value.p}}</label><label wx:else class="price free">公开课</label></view></view></view></view>